<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="top.woaibocai.bczx.manager.SysUserMapper">

    <!--  SysUser selectUserInfoByUserName(String dtoUserName);  -->
    <select id="selectUserInfoByUserName" resultType="top.woaibocai.bczx.model.entity.system.SysUser">
        select * from sys_user where username = #{userName} and is_deleted = 0
    </select>
</mapper>